{
  "name": "agape-care",
  "version": "1.0.0",
  "description": "Agape-Care â€“ Nursing home management system with admin ERP and public website",
  "private": true,
  "author": "Shiwoo Min",
  "license": "MIT",
  "type": "module",
  "workspaces": [
    "apps/*",
    "infra/*",
    "packages/*"
  ],
  "engines": {
    "node": ">=22.19.0",
    "pnpm": ">=9.0.0"
  },
  "packageManager": "pnpm@9.15.4",
  "scripts": {
    "dev": "vite",
    "dev:all": "pnpm -r --parallel dev",
    "dev:web": "pnpm --filter @agape-care/web dev",
    "dev:admin": "pnpm --filter @agape-care/admin dev",
    "dev:api": "pnpm --filter @agape-care/api dev",
    "dev:worker": "pnpm --filter @agape-care/worker dev",
    "build": "pnpm -r build",
    "build:web": "pnpm --filter @agape-care/web build",
    "build:admin": "pnpm --filter @agape-care/admin build",
    "build:api": "pnpm --filter @agape-care/api build",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"**/*.{ts,tsx,js,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,json,md}\"",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "db:seed": "tsx infra/database/seed.ts",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:watch": "vitest watch",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:bundle": "pnpm --filter @agape-care/web analyze",
    "clean": "pnpm -r exec rm -rf dist .next out coverage node_modules/.cache",
    "clean:all": "pnpm clean && rm -rf node_modules apps/*/node_modules pnpm-lock.yaml",
    "prepare": "husky",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "changeset publish",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "deps:check": "syncpack list-mismatches",
    "deps:fix": "syncpack fix-mismatches",
    "deps:unused": "depcheck"
  },
